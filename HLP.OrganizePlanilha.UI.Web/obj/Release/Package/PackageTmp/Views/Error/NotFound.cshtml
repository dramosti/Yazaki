@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "..:: Página não encontrada ::..";
    Layout = "~/Views/Shared/_LayoutPadrao.cshtml";
}

<div class="container">
    <div class="container">
        <div class="row clearfix">
            <div class="col-md-12 column">
                <blockquote>
                    <p>
                        <strong>:( , </strong>A Página solicitada não foi encontrada.
                    </p> <small>Favor verificar se a url foi digitada corretamente.</small>
                </blockquote>
            </div>
        </div>
    </div>
    @*&& HttpContext.Current.IsDebuggingEnabled*@
    @if (Model != null)
    {
        <div class="row clearfix">
            <div class="col-md-6 column">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">
                            <b>Exception:</b> @Model.Exception.Message<br />
                        </h3>
                    </div>
                    <div class="panel-body">
                        <b>Controller:</b> @Model.ControllerName<br />
                    </div>
                    <div class="panel-footer">
                        <b>Action:</b> @Model.ActionName
                    </div>
                </div>
            </div>
            <div class="col-md-6 column">
            </div>
        </div>
    }
    @if (Model != null)
    {
        <div class="row clearfix">
            <div class="col-md-12 column">
                <div class="panel-group" id="panel-854537">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <a class="panel-title" data-toggle="collapse" data-parent="#panel-854537" href="#panel-element-778388">Detalhes:</a>
                        </div>
                        <div id="panel-element-778388" class="panel-collapse collapse in">
                            <div class="panel-body">
                                @Model.Exception.StackTrace
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
</div>

